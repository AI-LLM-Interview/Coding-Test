   "source": [
    "def solution(people, limit):\n",
    "    people.sort()\n",
    "    answer = 0\n",
    "    start = 0\n",
    "    end = len(people) - 1\n",
    "    while start <= end:\n",
    "        if people[start] + people[end] <= limit:\n",
    "            start += 1\n",
    "        end -= 1\n",
    "        answer += 1\n",
    "    \n",
    "    return answer"
   ]
  }
